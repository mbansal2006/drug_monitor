            <section ID="ID_84a484e2-5ad7-414a-8b8a-013ab1a1226a">
               <id root="f111af4c-5304-4ffb-a71b-0e75fa58379d"/>
               <code code="42229-5" codeSystem="2.16.840.1.113883.6.1" displayName="SPL UNCLASSIFIED SECTION"/>
               <text>
                  <table width="40%">
                     <col width="39%"/>
                     <thead>
                        <tr>
                           <th align="center" styleCode="Rrule Botrule Lrule Toprule " valign="top">
                              <content styleCode="bold">CONCENTRATE</content>
