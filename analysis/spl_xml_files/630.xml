         <component>
            <section ID="s1">
               <id root="d06b0024-215b-4f5d-935d-7c45ebe9d518"/>
               <code code="42229-5" codeSystem="2.16.840.1.113883.6.1" displayName="SPL UNCLASSIFIED SECTION"/>
               <text>
                  <paragraph>
                     <content styleCode="bold">Rx only</content>
                  </paragraph>
               </text>
               <effectiveTime value="20211109"/>
            </section>
