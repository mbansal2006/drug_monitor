            <section ID="d7e1df4c-5257-49b1-bac7-4783ac13f276">
               <id root="3e37c3ef-e169-4a75-a312-944cbccff0d8"/>
               <code code="42229-5" codeSystem="2.16.840.1.113883.6.1" displayName="SPL UNCLASSIFIED SECTION"/>
               <text>
                  <paragraph>Injection, USP</paragraph>
                  <paragraph>
                     <content styleCode="bold">2 mEq/mL</content>
                  </paragraph>
                  <table width="100%">
                     <col width="98%"/>
                     <tbody>
                        <tr>
                           <td align="center" styleCode="Rrule Botrule Lrule Toprule " valign="middle">
                              <paragraph>
                                 <content styleCode="bold">Pharmacy Bulk Package.</content>
                              </paragraph>
                              <paragraph>
                                 <content styleCode="bold">Not for Direct Infusion</content>
                              </paragraph>
                           </td>
                        </tr>
                     </tbody>
                  </table>
                  <paragraph>
                     <content styleCode="bold">FOR ADDITIVE USE ONLY AFTER DILUTION IN INTRAVENOUS FLUIDS.</content>
                  </paragraph>
                  <paragraph>Glass Fliptop Vial </paragraph>
               </text>
               <effectiveTime value="20241108"/>
            </section>
