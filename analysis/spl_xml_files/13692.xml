            <section ID="L9515372e-0b8a-4d9e-a871-50b3b1de4fe4">
               <id root="2bdf8f55-9a64-bff7-e063-6394a90aafd0"/>
               <code code="42229-5" codeSystem="2.16.840.1.113883.6.1" displayName="SPL UNCLASSIFIED SECTION"/>
               <text>
                  <paragraph>
                     <content styleCode="bold">SODIUM ACETATE</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold"/>Injection, USP

 </paragraph>
                  <paragraph>
                     <content styleCode="bold">40 mEq (2 mEq/mL)</content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold">
                        <content styleCode="italics">FOR ADDITIVE USE ONLY AFTER DILUTION</content>
                     </content>
                  </paragraph>
                  <paragraph>
                     <content styleCode="bold">
                        <content styleCode="italics">IN INTRAVENOUS FLUIDS.</content>
                     </content>
                  </paragraph>
                  <br/>
                  <paragraph>Glass Fliptop Vial</paragraph>
                  <paragraph>Rx only</paragraph>
               </text>
               <effectiveTime value="20241021"/>
            </section>
